    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.0.2",
    "redux": "^4.2.0",
    "rxjs": "^7.5.6",
    "sequency": "^0.20.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.7.11",
    "@types/ramda": "^0.28.15",
    "@types/react": "^18.0.17",
